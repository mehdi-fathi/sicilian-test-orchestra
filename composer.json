{
    "name": "mehdi-fathi/sicilian-test-orchestra",
    "description": "An efficient composer package to test all of your routes with send request to make sure all things works well",
    "keywords": [
        "laravel",
        "laravel tools",
        "SicilianTestOrchestra",
        "Sicilian Test Orchestra",
        "feature tests",
        "tests",
        "laravel tests",
        "test laravel"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "SicilianTestOrchestra\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mehdi",
            "email": "mehdifathi.developer@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",

        "orchestra/testbench": "^8.0.0",
        "laravel/laravel" : "10.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test" : "vendor/bin/phpunit"
    }
}
